component {	function hello() {		// EXIT HANDLERS: 		rc.xehHello = "General.doHello";		rc.xehStartOver = "General.doStartOver";		// Do Your Logic Here		log.info("Starting dspHello. Using default name");		Event.setValue( "firstname", getSetting("Codename", true) & getSetting("Version", true) );		// Set the View To Display, after Logic		Event.setView( "general/hello" );		log.info("View has been set");	}			function doHello() {		// EXIT HANDLERS:		rc.xehHello = "General.doHello";		rc.xehStartOver = "General.doStartOver";		// Logger		log.info(Event.getValue("firstname"));		// Do Your Logic Here		if( Event.getValue( "firstname" ) == '' ) {			Event.setValue( "firstname", "Not Found" );		}		// Set the View To Display, after Logic		Event.setView( "General/helloRich" );	}		function doStartOver() { 		// Do Your Logic Here		setNextEvent( "General.hello" );	}		function popup() {		// Render a view with no layout.		Event.setView( view="general/popup", noLayout=true );	}	}