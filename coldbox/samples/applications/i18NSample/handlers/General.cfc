component {	property name='i18n' inject='i18n@cbi18n';	function onAppStart() {	}	function dspHome() {		//Vars		var hours = randRange(20,200);		// Param Values		event.paramValue("thisLocale", "th_TH");		event.paramValue("dateF", "0");		event.paramValue("timeF", "2");		//Change Locale		i18n.setfwLocale(rc.thisLocale);		//Set variables to display for locale.		rc.now = i18n.toEpoch(now());		rc.locales = i18n.getLocales();		rc.lang = i18n.getFWLanguage();		rc.country = i18n.getFWCountry();		rc.timeZones = i18n.getAvailableTZ();		rc.serverTZ = i18n.getServerTZ();		rc.thisTZ = rc.timezones[randRange(1,arrayLen(rc.timeZones))];		rc.laterOn = i18n.i18nDateAdd(rc.now,"hour",hours,rc.thisTZ);		rc.usesDST = i18n.usesDST(rc.thisTZ);		if (rc.usesDST)			rc.inDST=i18n.inDST(rc.now,rc.thisTZ);		rc.tDate=i18n.i18nDateFormat(rc.now,rc.dateF,rc.thisTZ);		rc.tDateTime=i18n.i18nDateTimeFormat(rc.now,rc.dateF,rc.timeF,rc.thisTZ);		rc.tzOffset=i18n.getTZOffset(rc.now,rc.thisTZ);       //Set the view       event.setView("Home");	}}