component {	property name="messageBox" inject="messageBox@cbmessageBox";	function home() {		//Exit Handlers		rc.xehUDFValidation = "General.doUseUDF";		//or use setValue("xehUDFValidation", "General.doUseUDF");		//Set the page's title		rc.title = "ColdBox - Sample UDF Library Usage.";	}	function doUseUDF() {		//Use the IsValidEmail() function on the UDF.cfm that was loaded		rc.validationResults = IsValidEmail( Event.getValue("email") );		//Set MessageBox accordingly		if ( rc.validationResults )			messageBox.setMessage("info","The email: #rc.email# is valid.");		else			messageBox.setMessage("error","The email: #rc.email# is not valid.");		//Run the Home		setNextEvent("General.Home");	}}