component {	property name='newsfeed' inject='webservice:newsfeed';	property name='messagebox' inject='messagebox@cbmessagebox';	property name='clientStorage' inject='ClientStorage@cbstorages';	function dspHome() {		rc.xehGetNews = "General.doGetNews";		rc.xehGetNewsClient = "General.doGetNewsClientVersion";		rc.xehDeleteNews = "General.doClearClientStorageNews";		// Set the View To Display, after Logic		Event.setView("Home");	}		function doGetNews() {		rc.xehGetNews = "General.doGetNews";		rc.xehGetNewsClient = "General.doGetNewsClientVersion";		rc.xehDeleteNews = "General.doClearClientStorageNews";		try{			//call model			rc.newsfeed = newsfeed.GetUpdateInfo("1.1.0","1.1.0");		}		catch(Any e){			log.error("Error retrieveing news",e);			messageBox.setMessage("error","Error retrieveing news feed. <br><br>Diagnostics:<br>#e.message#<br>#e.detail#");			//Set view and exit			Event.setView("Home");			return;		}		messageBox.setMessage("info","News retrieved and placed in request collection.");		//set the view		Event.setView("Home");	}		function doGetNewsClientVersion() {		//Model Call		var newsfeed = newsfeed.GetUpdateInfo("1.1.0","1.1.0");		//Using clientStorage to save variables permanently		clientStorage.setVar("newsfeed", newsfeed);		messagebox.setMessage( "info", "News retrieved using client storag." );		//Relocate to next Event, we can do this, since the data is in a storage.		setNextEvent("General.dspHome");	}		function doClearClientStorageNews() {		clientStorage.deleteVar("newsfeed");		messagebox.setMessage( "info", "News deleted from client storage." );		//Relocate to next Event, we can do this, since the data is in a storage.		setNextEvent("General.dspHome");	}}