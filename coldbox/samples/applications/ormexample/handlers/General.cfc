component {	property name="authorService" inject="entityService:Author";		public void function index() {				prc.selectedNav = "general.index";				event.setView("home");	}		function basic() {				prc.selectedNav = "general.basic";				var c = authorService.newCriteria();		prc.example1 = c.eq( "firstName", "Michael" )			.list( asQuery=true );				var c = authorService.newCriteria();		prc.example2 = c.ne( "firstName", "Michael" )			.list( asQuery=true );				var c = authorService.newCriteria();		prc.example3a = c.in( "firstName", [ "Ian", "Emily", "Paul" ] )			.list( asQuery=true );				var c = authorService.newCriteria();		prc.example3b = c.in( "id", JavaCast( "java.lang.Integer[]", [ 2, 5, 9 ] ) )			.list( asQuery=true );				var c = authorService.newCriteria();		prc.example4 = c.like( "lastName", "M%" )			.list( asQuery=true );				var c = authorService.newCriteria();		prc.example5 = c.ilike( "lastName", "s%" )			.list( asQuery=true );				var c = authorService.newCriteria();		prc.example6 = c.isEmpty( "books" )			.list( asQuery=true );				var c = authorService.newCriteria();		prc.example7 = c.isNotEmpty( "books" )			.list( asQuery=true );				var c = authorService.newCriteria();		prc.example8 = c.isNull( "bio" )			.list( asQuery=true );				var c = authorService.newCriteria();		prc.example9 = c.isNotNull( "bio" )			.list( asQuery=true );				var c = authorService.newCriteria();		prc.example10a = c.between( "lastName", "A", "M" )			.list( asQuery=true );				var c = authorService.newCriteria();		prc.example10b = c.between( "id", JavaCast( "int", 3 ), JavaCast( "int", 7 ) )			.list( asQuery=true );				var c = authorService.newCriteria();		prc.example11 = c.gt( "id", JavaCast( "int", 8 ) )			.list( asQuery=true );				var c = authorService.newCriteria();		prc.example12 = c.ge( "id", JavaCast( "int", 13 ) )			.list( asQuery=true );		event.setView("basic");	}		function advanced() {				prc.selectedNav = "general.advanced";				var c = authorService.newCriteria();		prc.example1 = c.or(			c.restrictions.eq("firstName","Emily"),			c.restrictions.eq("firstName","Paul"),			c.restrictions.eq("firstName","Amy")		)		.list( asQuery=true );				var c = authorService.newCriteria();		prc.example2 = c.gt( "id", JavaCast( "int", 7 ) )		.$or(			c.restrictions.eq("firstName","Emily"),			c.restrictions.eq("firstName","Paul"),			c.restrictions.eq("firstName","Amy")		)		.list( asQuery=true );				event.setView("advanced");	}	}